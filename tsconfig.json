{
  "compilerOptions": {
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "outDir": "./public/",
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "ES2017"
    ],
    "module": "ES2015",
    "target": "es5",
    "jsx": "react",
    "newLine": "lf"
  }
}